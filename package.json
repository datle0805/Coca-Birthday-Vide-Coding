{
  "name": "my-backend-app",
  "version": "1.0.0",
  "description": "A backend application using Node.js, MongoDB, and Express.",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "ts-node-dev src/index.ts",
    "docker:build": "docker build -t vibe-backend .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f backend",
    "docker:rebuild": "docker-compose down && docker-compose up -d --build"
  },
  "dependencies": {
    "cloudinary": "^1.41.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.10.9"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.5.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "license": "MIT"
}
